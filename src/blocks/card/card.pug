mixin card({ text='', links = []}={}) 
  include ../social/social.pug   
  div.card
    span.card__sign 
      span.card__logo
        include ../../assets/images/logo.img_color.svg
      span.card__brand
        include ../../assets/images/TOXIN.svg
    span.card__subscribe #{text}
  
    span.card__social
      +social(links)