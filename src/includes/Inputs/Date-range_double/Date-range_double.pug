mixin Date-range_double(issues)
    .date-range_double
        div(class=`date-range_double__container`)
            include ../../Figures/Arrow-down/Arrow-down.pug
            label 
                if issues[0].label    
                    h3.date-range_double__subscribe #{issues[0].label}
                input(class="date-range_double__value date-range_double__value_first js-date-range_double__value_first" placeholder=`${issues[0].placeholder}` id=`${Math.random()}`)
        div(class=`date-range_double__container`)
            include ../../Figures/Arrow-down/Arrow-down.pug
            label 
                if issues[1].label      
                    h3.date-range_double__subscribe #{issues[1].label}
                input(class="date-range_double__value date-range_double__value_second js-date-range_double__value_second" placeholder=`${issues[1].placeholder}` id=`${Math.random()}`)