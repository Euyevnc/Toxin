{
  "parser": "babel-eslint",
  "env": { 
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "ignorePatterns": ["/dist/**/*.js","/src/plugins/*.js"],
  
  "extends": [ "airbnb"],
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": false }],
    "max-classes-per-file": ["off"] 
  }
}