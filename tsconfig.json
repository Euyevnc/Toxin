{
  "compilerOptions": {
    "module": "commonjs",
    "charset": "utf8",
    "outDir": "./dist/",
    "target": "es5",
    "allowJs": true,
    "checkJs": false, 
    "sourceMap": false,
    "declaration": false,
    "noEmit": false, 
    "isolatedModules": false,

    "alwaysStrict": true,
    "strictNullChecks": false, 
    "noImplicitAny": true,

    "noUnusedLocals": true,
    "noUnusedParameters": false,

    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    
  },
}
